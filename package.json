  "scripts": {
    "android": "ns run android --no-hmr",
    "ios": "ns run ios --no-hmr",
    "mobile": "ns run",
    "ng": "ng",
    "start": "ng serve --port=4201",
    "develop": "ng serve --port=4201 --configuration=local",
    "develop:ssl": "ng serve --port=4201 --configuration=local --ssl true",
    "develop:dev": "ng serve --port=4201 --configuration=dev",
    "develop:prod": "ng serve --port=4201 --configuration=production",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --prod --output-hashing=all",
    "build:dev": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=dev --output-hashing=all",
    "build:app": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=app --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "eslint": "eslint --ext .tsx,.ts --fix ./src"
  },
