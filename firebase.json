{
  "hosting": [
    {
      "target": "lifo-influencer",
      "public": "dist/influencer",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
